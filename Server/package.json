{
  "name": "bigcity-server",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec tsx src/server.ts",
    "dev:js": "nodemon dist/server.js",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "node dist/server.js",
    "clean": "rm -rf dist",
    "build:clean": "npm run clean && npm run build",
    "preview": "npm run build && npm run start"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^4.18.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.4",
    "morgan": "^1.10.0",
    "mysql2": "^3.15.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.16.2",
    "uuid": "^11.1.0",
    "validator": "^13.15.15"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.96",
    "@types/morgan": "^1.9.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.15.2",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
